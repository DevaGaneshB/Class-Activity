<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from AnimalTypeServiceService WSDL service -->

  <service name="AnimalTypeService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://webservice"/>
      <parameter name="wsdlServiceElement" value="AnimalTypeServiceService"/>
      <parameter name="schemaQualified" value="http://webservice"/>
      <parameter name="wsdlServicePort" value="AnimalTypeService"/>
      <parameter name="className" value="webservice.AnimalTypeServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="AnimalTypeService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="animalType" qname="operNS:animalType" xmlns:operNS="http://webservice" returnQName="retNS:animalTypeReturn" xmlns:retNS="http://webservice" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:animal" xmlns:pns="http://webservice" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="animalType"/>

  </service>
</deployment>
